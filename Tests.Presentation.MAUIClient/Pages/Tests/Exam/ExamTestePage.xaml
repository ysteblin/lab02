<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Tests.Presentation.MAUIClient.Pages.Tests.Exam.ExamTestPage"
             xmlns:viewModels="clr-namespace:Tests.Presentation.MAUIClient.ViewModels.Tests.Exam"
             x:DataType="viewModels:ExamTestViewModel"
             Title="ExamTestPage">
    <VerticalStackLayout>
        <Button 
            Text="Добавить объект" 
            SemanticProperties.Hint="Добавить объект"
            Command="{Binding GoToAddExamTestPageCommand}" 
            HorizontalOptions="Fill" 
            Margin="20"/>

        <Button 
            Text="Список объектов" 
            SemanticProperties.Hint="Список объектов"
            Command="{Binding GoToGetExamTestPageCommand}" 
            HorizontalOptions="Fill" 
            Margin="20"/>
    </VerticalStackLayout>
</ContentPage>